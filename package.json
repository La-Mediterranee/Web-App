{
	"name": "food-delivery",
	"version": "0.2.0",
	"type": "module",
	"license": "MIT",
	"scripts": {
		"dev": "npm-run-all --parallel sveltekit typesafe-i18n",
		"dev:check": "npm-run-all --parallel sveltekit typesafe-i18n check:watch",
		"build": "npm run build:remove && svelte-kit build",
		"build:remove": "rm -rf ./build",
		"build:preview": "npm run build && svelte-kit preview",
		"preview": "svelte-kit preview",
		"check": "svelte-check --tsconfig ./tsconfig.json --threshold error",
		"check:watch": "svelte-check --tsconfig ./tsconfig.json --watch --threshold error --ignore .svelte-kit",
		"typesafe-i18n": "typesafe-i18n",
		"sveltekit": "svelte-kit dev",
		"lint": "eslint --ignore-path .gitignore ."
	},
	"devDependencies": {
		"@babel/core": "^7.16.12",
		"@babel/preset-env": "^7.16.11",
		"@babel/preset-typescript": "^7.16.7",
		"@inlang/typesafe-i18n-importer": "^0.1.8",
		"@mdi/js": "^6.5.95",
		"@stripe/stripe-js": "^1.22.0",
		"@sveltejs/adapter-cloudflare-workers": "next",
		"@sveltejs/adapter-netlify": "next",
		"@sveltejs/adapter-node": "next",
		"@sveltejs/adapter-static": "next",
		"@sveltejs/adapter-vercel": "next",
		"@sveltejs/kit": "^1.0.0-next.240",
		"@types/google.analytics": "^0.0.42",
		"@types/gtag.js": "^0.0.8",
		"@types/jest": "^27.4.0",
		"@typescript-eslint/eslint-plugin": "^5.10.0",
		"@typescript-eslint/parser": "^5.10.0",
		"autoprefixer": "^10.4.2",
		"dotenv": "^10.0.0",
		"eslint": "^8.7.0",
		"eslint-plugin-svelte3": "^3.4.0",
		"firebase": "^9.6.4",
		"firebase-admin": "^10.0.2",
		"idb": "7.0.0",
		"jest": "^27.4.7",
		"jwt-decode": "^3.1.2",
		"npm-run-all": "^4.1.5",
		"postcss": "^8.4.5",
		"sass": "1.45.0",
		"schema-dts": "^1.0.0",
		"svelte": "^3.46.2",
		"svelte-check": "^2.3.0",
		"svelte-preprocess": "^4.10.2",
		"svelty-material": "^1.0.32",
		"tslib": "^2.3.1",
		"typesafe-i18n": "^2.60.1",
		"typescript": "^4.5.5",
		"vite": "^2.7.13"
	}
}
