{
	"compilerOptions": {
		"module": "commonjs",
		"noImplicitReturns": true,
		"noUnusedLocals": true,
		"noImplicitAny": false,
		"outDir": "lib",
		"sourceMap": true,
		"strict": true,
		"target": "es2017",
		"lib": ["ESNext"],
		"strictNullChecks": false,
		"esModuleInterop": true,
		"paths": {
			"@consts": ["./src/util/consts.ts"]
		}
	},
	"compileOnSave": true,
	"include": ["src"]
}
